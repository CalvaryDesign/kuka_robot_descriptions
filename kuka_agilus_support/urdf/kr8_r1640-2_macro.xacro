        <geometry>
          <mesh filename="package://kuka_agilus_support/meshes/kr8_r1640-2/visual/link_1.STL" />
        </geometry>
        <xacro:material_kuka_orange />
      </visual>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
          <mesh filename="package://kuka_agilus_support/meshes/kr8_r1640-2/visual/link_1.STL" />
        </geometry>
      </collision>
      <inertial>
        <origin
          xyz="0.0632284832510284 0.00929091089013534 0.151469624605647"
          rpy="0 0 0" />
        <mass
          value="16.5877043278261" />
        <inertia
          ixx="0.213364303503939"
          ixy="-0.0012485292563484"
          ixz="-0.0652741773214894"
          iyy="0.271565940766349"
          iyz="-0.00700452845831783"
          izz="0.280770500243086" />
      </inertial>
    </link>

    <link name="${prefix}link_2">
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
          <mesh filename="package://kuka_agilus_support/meshes/kr8_r1640-2/visual/link_2.STL" />
        </geometry>
        <xacro:material_kuka_orange />
      </visual>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
          <mesh filename="package://kuka_agilus_support/meshes/kr8_r1640-2/visual/link_2.STL" />
        </geometry>
      </collision>
      <inertial>
        <origin
          xyz="0.00139980750670374 -0.171929998547248 0.373062489097034"

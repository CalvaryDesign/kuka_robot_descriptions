      </inertial>
    </link>

    <link name="${prefix}link_4">
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
          <mesh filename="package://kuka_agilus_support/meshes/kr8_r1640-2/visual/link_4.STL" />
        </geometry>
        <xacro:material_kuka_orange />
      </visual>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
          <mesh filename="package://kuka_agilus_support/meshes/kr8_r1640-2/visual/link_4.STL" />
        </geometry>
      </collision>
      <inertial>
        <origin
          xyz="0.166030519876032 0.00351269770895513 -0.000101995036007274"
          rpy="0 0 0" />
        <mass
          value="3.65024761849689" />
        <inertia
          ixx="0.00917542298020903"
          ixy="-0.00142087192898842"
          ixz="3.77390163901273E-05"
          iyy="0.0307659104375414"
          iyz="2.35686653555563E-06"
          izz="0.0341691417151628" />
      </inertial>
    </link>

    <link name="${prefix}link_5">
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
          <mesh filename="package://kuka_agilus_support/meshes/kr8_r1640-2/visual/link_5.STL" />
        </geometry>
        <xacro:material_kuka_orange />
      </visual>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
          <mesh filename="package://kuka_agilus_support/meshes/kr8_r1640-2/visual/link_5.STL" />
        </geometry>
      </collision>
      <inertial>
        <origin
          xyz="0.0193967415674676 5.88454126617677E-05 -4.60295850346348E-06"
